    "test:i": "TS_NODE_PROJECT=tests/tsconfig.json electron-mocha --interactive --opts .mocha",
    "test:si": "TS_NODE_PROJECT=tests/tsconfig.json electron-mocha --interactive --opts .mocha.single",
